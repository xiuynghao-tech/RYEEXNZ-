-- Pastikan library Scoot UI dimuat (sesuai URL yang Anda berikan)
loadstring(game:HttpGet("https://raw.githubusercontent.com/x2Swiftz/UI-Library/refs/heads/main/Libraries/Scoot%20UI%20-%20Example.lua"))()

-- Tunggu sebentar hingga library siap
task.wait(0.5)

-- Periksa apakah library berhasil dimuat
if not Library then
    warn("Scoot UI gagal dimuat! Periksa koneksi atau sumber library.")
    return
end

-- ==================================================
-- MEMBUAT WINDOW UTAMA
-- ==================================================
local Window = Library:CreateWindow({
    Name = "RYEENZY EXPLOIT",
    Subtitle = "The Ultimate Roblox Exploit",
    Size = UDim2.new(0, 700, 0, 450),
    Position = UDim2.new(0.5, -350, 0.5, -225),
    Theme = "Preset", -- Menggunakan tema preset dari library
    Keybind = Enum.KeyCode.RightControl -- Tombol untuk membuka/tutup GUI
})

-- Jika Window tidak berhasil dibuat, buat secara manual dengan instance dasar
if not Window then
    -- Fallback: Buat GUI sederhana jika Window gagal
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "RYEENZY_Exploit"
    screenGui.Parent = gethui and gethui() or game:GetService("CoreGui")
    screenGui.ResetOnSpawn = false
    
    local frame = Instance.new("Frame")
    frame.Parent = screenGui
    frame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
    frame.BorderSizePixel = 0
    frame.Position = UDim2.new(0.5, -300, 0.5, -200)
    frame.Size = UDim2.new(0, 600, 0, 400)
    frame.Active = true
    frame.Draggable = true
    
    local title = Instance.new("TextLabel")
    title.Parent = frame
    title.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
    title.BorderSizePixel = 0
    title.Size = UDim2.new(1, 0, 0, 30)
    title.Font = Enum.Font.GothamBold
    title.Text = "RYEENZY EXPLOIT (Fallback Mode)"
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.TextSize = 14
    
    print("âš ï¸ Scoot UI Window gagal, menggunakan fallback GUI")
    return
end

-- ==================================================
-- MEMBUAT TAB-TAB UTAMA
-- ==================================================
local TabMain = Window:CreateTab({
    Name = "Main",
    Icon = "rbxassetid://6031068350" -- Ikon gear
})

local TabPlayer = Window:CreateTab({
    Name = "Player",
    Icon = "rbxassetid://6031075930" -- Ikon player
})

local TabCombat = Window:CreateTab({
    Name = "Combat",
    Icon = "rbxassetid://6031083096" -- Ikon pedang
})

local TabVisual = Window:CreateTab({
    Name = "Visuals",
    Icon = "rbxassetid://6031091635" -- Ikon mata
})

local TabMisc = Window:CreateTab({
    Name = "Misc",
    Icon = "rbxassetid://6031097357" -- Ikon lainnya
})

-- ==================================================
-- SECTION: MAIN TAB
-- ==================================================
local MainSection1 = TabMain:CreateSection("Auto Farm")
local MainSection2 = TabMain:CreateSection("Auto Collect")
local MainSection3 = TabMain:CreateSection("Auto Sell")

-- Variabel global untuk kontrol fitur
_G.RYEENZY = {
    AutoFarm = false,
    AutoCollect = false,
    AutoSell = false,
    WalkSpeed = 16,
    JumpPower = 50,
    InfiniteJump = false,
    Aimbot = false,
    ESP = false,
    Fullbright = false,
    Noclip = false
}

-- Auto Farm Toggle
MainSection1:Toggle({
    Name = "Auto Farm",
    Description = "Otomatis menyerang musuh terdekat",
    Flag = "AutoFarm",
    Default = false,
    Callback = function(state)
        _G.RYEENZY.AutoFarm = state
        if state then
            spawn(function()
                while _G.RYEENZY.AutoFarm and Window.Visible do
                    pcall(function()
                        local player = game.Players.LocalPlayer
                        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            -- Cari musuh terdekat
                            local nearestEnemy = nil
                            local nearestDist = math.huge
                            
                            for _, v in pairs(workspace:GetDescendants()) do
                                if v:IsA("Model") and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") then
                                    if v ~= player.Character and v.Humanoid.Health > 0 then
                                        local dist = (v.HumanoidRootPart.Position - player.Character.HumanoidRootPart.Position).Magnitude
                                        if dist < nearestDist then
                                            nearestDist = dist
                                            nearestEnemy = v
                                        end
                                    end
                                end
                            end
                            
                            if nearestEnemy then
                                player.Character.HumanoidRootPart.CFrame = nearestEnemy.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
                                -- Simulasi serangan
                                if player.Character:FindFirstChildOfClass("Tool") then
                                    local tool = player.Character:FindFirstChildOfClass("Tool")
                                    tool:Activate()
                                end
                            end
                        end
                    end)
                    wait(1.5)
                end
            end)
        end
    end
})

-- Slider Kecepatan Farm
MainSection1:Slider({
    Name = "Farm Interval",
    Description = "Kecepatan loop auto farm (detik)",
    Min = 0.5,
    Max = 5,
    Default = 1.5,
    Flag = "FarmSpeed",
    Callback = function(value)
        _G.RYEENZY.FarmSpeed = value
    end
})

-- Auto Collect Toggle
MainSection2:Toggle({
    Name = "Auto Collect",
    Description = "Otomatis mengumpulkan item/drop",
    Flag = "AutoCollect",
    Default = false,
    Callback = function(state)
        _G.RYEENZY.AutoCollect = state
        if state then
            spawn(function()
                while _G.RYEENZY.AutoCollect and Window.Visible do
                    pcall(function()
                        local player = game.Players.LocalPlayer
                        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            -- Cari item yang bisa dikoleksi
                            for _, v in pairs(workspace:GetDescendants()) do
                                if v:IsA("Part") and (v.Name:lower():find("ore") or v.Name:lower():find("drop") or v.Name:lower():find("item")) then
                                    if (v.Position - player.Character.HumanoidRootPart.Position).Magnitude < 50 then
                                        player.Character.HumanoidRootPart.CFrame = v.CFrame + Vector3.new(0, 3, 0)
                                        wait(0.3)
                                        -- Simulasi collect (trigger proximity prompt jika ada)
                                        if v:FindFirstChildOfClass("ProximityPrompt") then
                                            fireproximityprompt(v:FindFirstChildOfClass("ProximityPrompt"))
                                        end
                                        break
                                    end
                                end
                            end
                        end
                    end)
                    wait(2)
                end
            end)
        end
    end
})

-- Auto Sell Toggle
MainSection3:Toggle({
    Name = "Auto Sell",
    Description = "Otomatis menjual item ke NPC",
    Flag = "AutoSell",
    Default = false,
    Callback = function(state)
        _G.RYEENZY.AutoSell = state
        if state then
            spawn(function()
                while _G.RYEENZY.AutoSell and Window.Visible do
                    pcall(function()
                        local player = game.Players.LocalPlayer
                        if player:FindFirstChild("PlayerGui") then
                            -- Cari tombol jual di GUI
                            for _, v in pairs(player.PlayerGui:GetDescendants()) do
                                if v:IsA("TextButton") then
                                    local text = v.Text:lower()
                                    if text == "sell" or text:find("jual") or text:find("sell") then
                                        v:Click()
                                        break
                                    end
                                end
                            end
                        end
                    end)
                    wait(3)
                end
            end)
        end
    end
})

-- ==================================================
-- SECTION: PLAYER TAB
-- ==================================================
local PlayerSection = TabPlayer:CreateSection("Player Modifications")

-- Slider WalkSpeed
PlayerSection:Slider({
    Name = "WalkSpeed",
    Description = "Atur kecepatan jalan",
    Min = 16,
    Max = 200,
    Default = 16,
    Flag = "WalkSpeed",
    Callback = function(value)
        _G.RYEENZY.WalkSpeed = value
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
            player.Character.Humanoid.WalkSpeed = value
        end
    end
})

-- Slider JumpPower
PlayerSection:Slider({
    Name = "JumpPower",
    Description = "Atur kekuatan lompat",
    Min = 50,
    Max = 200,
    Default = 50,
    Flag = "JumpPower",
    Callback = function(value)
        _G.RYEENZY.JumpPower = value
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
            player.Character.Humanoid.JumpPower = value
        end
    end
})

-- Toggle Infinite Jump
PlayerSection:Toggle({
    Name = "Infinite Jump",
    Description = "Lompat tanpa batas",
    Flag = "InfiniteJump",
    Default = false,
    Callback = function(state)
        _G.RYEENZY.InfiniteJump = state
        local UIS = game:GetService("UserInputService")
        
        if state then
            if _G.InfJumpConnection then
                _G.InfJumpConnection:Disconnect()
            end
            _G.InfJumpConnection = UIS.JumpRequest:Connect(function()
                if _G.RYEENZY.InfiniteJump and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
                    game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                end
            end)
        else
            if _G.InfJumpConnection then
                _G.InfJumpConnection:Disconnect()
                _G.InfJumpConnection = nil
            end
        end
    end
})

-- Toggle Noclip
PlayerSection:Toggle({
    Name = "Noclip",
    Description = "Tembus dinding",
    Flag = "Noclip",
    Default = false,
    Callback = function(state)
        _G.RYEENZY.Noclip = state
        spawn(function()
            while _G.RYEENZY.Noclip and Window.Visible do
                pcall(function()
                    local player = game.Players.LocalPlayer
                    if player.Character then
                        for _, v in pairs(player.Character:GetDescendants()) do
                            if v:IsA("BasePart") and v.CanCollide then
                                v.CanCollide = false
                            end
                        end
                    end
                end)
                wait(0.1)
            end
        end)
    end
})

-- Tombol Teleport ke Spawn
PlayerSection:Button({
    Name = "Teleport to Spawn",
    Description = "Teleport ke titik spawn",
    Callback = function()
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 50, 0)
        end
    end
})

-- ==================================================
-- SECTION: COMBAT TAB
-- ==================================================
local CombatSection = TabCombat:CreateSection("Combat Options")

-- Toggle Aimbot
CombatSection:Toggle({
    Name = "Aimbot",
    Description = "Aktifkan aimbot (Lock target)",
    Flag = "Aimbot",
    Default = false,
    Callback = function(state)
        _G.RYEENZY.Aimbot = state
    end
})

-- Slider Aimbot FOV
CombatSection:Slider({
    Name = "Aimbot FOV",
    Description = "Field of View untuk aimbot",
    Min = 50,
    Max = 500,
    Default = 150,
    Flag = "AimbotFOV",
    Callback = function(value)
        _G.RYEENZY.AimbotFOV = value
    end
})

-- Dropdown untuk memilih bagian tubuh
CombatSection:Dropdown({
    Name = "Aimbot Part",
    Description = "Target bagian tubuh",
    Options = {"Head", "Torso", "HumanoidRootPart", "Left Arm", "Right Arm", "Left Leg", "Right Leg"},
    Default = "Head",
    Flag = "AimbotPart",
    Callback = function(option)
        _G.RYEENZY.AimbotPart = option
    end
})

-- Tombol Instant Kill (simulasi)
CombatSection:Button({
    Name = "Instant Kill (Test)",
    Description = "Set damage tool menjadi besar",
    Callback = function()
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChildOfClass("Tool") then
            local tool = player.Character:FindFirstChildOfClass("Tool")
            -- Cari value damage
            for _, v in pairs(tool:GetDescendants()) do
                if v:IsA("NumberValue") and (v.Name:lower():find("damage") or v.Name:lower():find("dmg")) then
                    v.Value = 999999
                    Library:Notify("Damage set ke 999999!", 2)
                    return
                end
            end
            Library:Notify("Tidak menemukan value damage!", 2)
        else
            Library:Notify("Tidak ada tool di tangan!", 2)
        end
    end
})

-- ==================================================
-- SECTION: VISUALS TAB
-- ==================================================
local VisualSection = TabVisual:CreateSection("ESP Settings")

-- Toggle ESP
VisualSection:Toggle({
    Name = "ESP Players",
    Description = "Lihat player melalui dinding",
    Flag = "ESP",
    Default = false,
    Callback = function(state)
        _G.RYEENZY.ESP = state
        if state then
            spawn(function()
                while _G.RYEENZY.ESP and Window.Visible do
                    pcall(function()
                        for _, v in pairs(game.Players:GetPlayers()) do
                            if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("Head") then
                                if not v.Character.Head:FindFirstChild("RYEENZY_ESP") then
                                    local bill = Instance.new("BillboardGui")
                                    bill.Name = "RYEENZY_ESP"
                                    bill.Parent = v.Character.Head
                                    bill.AlwaysOnTop = true
                                    bill.Size = UDim2.new(0, 150, 0, 40)
                                    bill.StudsOffset = Vector3.new(0, 2, 0)
                                    
                                    local bg = Instance.new("Frame")
                                    bg.Parent = bill
                                    bg.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                                    bg.BackgroundTransparency = 0.3
                                    bg.Size = UDim2.new(1, 0, 1, 0)
                                    
                                    local nameLabel = Instance.new("TextLabel")
                                    nameLabel.Parent = bg
                                    nameLabel.BackgroundTransparency = 1
                                    nameLabel.Size = UDim2.new(1, 0, 0.6, 0)
                                    nameLabel.Font = Enum.Font.GothamBold
                                    nameLabel.Text = v.Name
                                    nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    nameLabel.TextSize = 14
                                    
                                    local distLabel = Instance.new("TextLabel")
                                    distLabel.Parent = bg
                                    distLabel.BackgroundTransparency = 1
                                    distLabel.Position = UDim2.new(0, 0, 0.6, 0)
                                    distLabel.Size = UDim2.new(1, 0, 0.4, 0)
                                    distLabel.Font = Enum.Font.Gotham
                                    distLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
                                    distLabel.TextSize = 10
                                    
                                    -- Update jarak setiap saat
                                    spawn(function()
                                        while _G.RYEENZY.ESP and bill and bill.Parent do
                                            if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                                                local dist = (v.Character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                                                distLabel.Text = string.format("%.1f studs", dist)
                                            end
                                            wait(0.5)
                                        end
                                    end)
                                end
                            end
                        end
                    end)
                    wait(1)
                end
                -- Hapus semua ESP saat dimatikan
                for _, v in pairs(game.Players:GetPlayers()) do
                    if v.Character and v.Character:FindFirstChild("Head") and v.Character.Head:FindFirstChild("RYEENZY_ESP") then
                        v.Character.Head.RYEENZY_ESP:Destroy()
                    end
                end
            end)
        else
            for _, v in pairs(game.Players:GetPlayers()) do
                if v.Character and v.Character:FindFirstChild("Head") and v.Character.Head:FindFirstChild("RYEENZY_ESP") then
                    v.Character.Head.RYEENZY_ESP:Destroy()
                end
            end
        end
    end
})

-- Toggle Fullbright
VisualSection:Toggle({
    Name = "Fullbright",
    Description = "Terang total",
    Flag = "Fullbright",
    Default = false,
    Callback = function(state)
        _G.RYEENZY.Fullbright = state
        if state then
            game:GetService("Lighting").Brightness = 2
            game:GetService("Lighting").GlobalShadows = false
            game:GetService("Lighting").FogEnd = 100000
        else
            game:GetService("Lighting").Brightness = 1
            game:GetService("Lighting").GlobalShadows = true
            game:GetService("Lighting").FogEnd = 1000
        end
    end
})

