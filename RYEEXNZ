-- ==================================================
-- LOOP UNTUK AIMBOT (SEDERHANA)
-- ==================================================
spawn(function()
    while Window.Visible do
        if _G.RYEENZY.Aimbot then
            pcall(function()
                local player = game.Players.LocalPlayer
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") and camera then
                    local closestTarget = nil
                    local closestDist = _G.RYEENZY.AimbotFOV or 150
                    
                    for _, v in pairs(game.Players:GetPlayers()) do
                        if v ~= player and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChildOfClass("Humanoid") and v.Character.Humanoid.Health > 0 then
                            local screenPos, onScreen = camera:WorldToViewportPoint(v.Character.HumanoidRootPart.Position)
                            if onScreen then
                                local dist = (Vector2.new(Mouse.X, Mouse.Y) - Vector2.new(screenPos.X, screenPos.Y)).Magnitude
                                if dist < closestDist then
                                    closestDist = dist
                                    closestTarget = v
                                end
                            end
                        end
                    end
                    
                    if closestTarget and closestTarget.Character and closestTarget.Character:FindFirstChild(_G.RYEENZY.AimbotPart or "Head") then
                        local targetPart = closestTarget.Character[_G.RYEENZY.AimbotPart or "Head"]
                        if targetPart then
                            -- Aim ke target
                            camera.CFrame = CFrame.new(camera.CFrame.Position, targetPart.Position)
                        end
                    end
                end
            end)
        end
        wait()
    end
end)
