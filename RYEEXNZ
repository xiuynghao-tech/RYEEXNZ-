local MiscSection = TabMisc:CreateSection("Utilities")

-- Tombol Rejoin
MiscSection:Button({
    Name = "Rejoin Game",
    Description = "Masuk ulang ke server yang sama",
    Callback = function()
        local ts = game:GetService("TeleportService")
        ts:Teleport(game.PlaceId, game.Players.LocalPlayer)
    end
})

-- Tombol Server Hop
MiscSection:Button({
    Name = "Server Hop",
    Description = "Pindah ke server lain",
    Callback = function()
        Library:Notify("Mencari server lain...", 2)
        -- Implementasi server hop sederhana
        local placeId = game.PlaceId
        local url = "https://games.roblox.com/v1/games/" .. placeId .. "/servers/Public?limit=10"
        local success, response = pcall(function()
            return game:HttpGet(url)
        end)
        
        if success and response then
            -- Parse JSON sederhana (tanpa library JSON)
            Library:Notify("Server hop dalam pengembangan!", 2)
        end
    end
})

-- Tombol Copy Telegram
MiscSection:Button({
    Name = "ðŸ“± Telegram",
    Description = "Copy link Telegram",
    Callback = function()
        if setclipboard then
            setclipboard("t.me/Swill_Way")
            Library:Notify("Telegram link copied!", 1)
        else
            Library:Notify("Executor tidak mendukung setclipboard", 2)
        end
    end
})

-- Tombol Destroy GUI
