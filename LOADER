-- Loader untuk THE FORGE SCRIPT
local scriptURL = "https://raw.githubusercontent.com/xiuynghao-tech/RYEEXNZ-/refs/heads/main/RYEEXNZ"

local function loadScript()
    local success, result = pcall(function()
        return game:HttpGet(scriptURL)
    end)

    if success and result then
        local loadSuccess, scriptError = pcall(loadstring, result)
        if loadSuccess and scriptError then
            scriptError()
            game.StarterGui:SetCore("SendNotification", {
                Title = "THE FORGE SCRIPT",
                Text = "Loaded successfully!",
                Duration = 3
            })
        else
            warn("Load Error: " .. tostring(scriptError))
        end
    else
        warn("Fetch Error: " .. tostring(result))
    end
end

loadScript()
